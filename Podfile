# platform :ios, '9.0'

target 'FIREfighter' do
  # Comment this line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FIREfighter
  pod 'Firebase/Core', '>=2.5.1'
  pod 'Firebase/Database'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'
  pod 'FBSDKLoginKit'

  target 'FIREfighterTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'FIREfighterUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
